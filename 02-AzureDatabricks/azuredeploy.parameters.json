{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "eventHubNamespaceName": {
      "value": "<eventHubNameSpace>"
    },
    "eventhubSku": {
      "value": "Standard"
    },
    "isAutoInflateEnabled": {
      "value": "True"
    },
    "maximumThroughputUnits": {
      "value": 10
    },
    "eventHubName": {
      "value": "<eventHubName>"
    },
    "messageRetentionInDays": {
      "value": 1
    },
    "partitionCount": {
      "value": 4
    },
    "captureEnabled": {
      "value": "true"
    },
    "captureEncodingFormat": {
      "value": "Avro"
    },
    "captureTime": {
      "value": 300
    },
    "captureSize": {
      "value": 314572800
    },
    "storageAccountName": {
      "value": "<storageAccountName>"
    },
    "blobContainerName": {
      "value": "data"
    },
    "captureNameFormat": {
      "value": "{Namespace}/{EventHub}/{PartitionId}/{Year}/{Month}/{Day}/{Hour}/{Minute}/{Second}"
    },
    "uniqueNameFix": {
      "value": "<uniqueNameFix>"
    },
    "projectName": {
      "value": "<projectName>"
    },
    "bootstrapStorageAccount": {
      "value": "<bootstrapStorageAccount>"
    },
    "bootstrapStorageAccountSas": {
      "value": "<bootstrapStorageAccountSas>"
    },
    "bootstrapStorageAccountUrl": {
      "value": "<bootstrapStorageAccountUrl>"
    },
    "workspaceName": {
      "value": "<workspaceName>"
    }
  }
}