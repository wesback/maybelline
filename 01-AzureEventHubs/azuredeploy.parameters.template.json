    "apiVersions": {
      "value": {
        "appInsights": "<appInsightsApiVersion>",
        "applicationGateways": "<applicationGatewaysApiVersion>",
        "autoScaleSettings": "<autoScaleSettingsApiVersion>",
        "deployments": "<deploymentsApiVersion>",
        "images": "<imagesApiVersion>",
        "loadBalancers": "<loadBalancersApiVersion>",
        "networkInterfaces": "<networkInterfacesApiVersion>",
        "networkSecurityGroups": "<networkSecurityGroupsApiVersion>",
        "publicIPAddresses": "<publicIPAddressesApiVersion>",
        "serverFarms": "<serverFarmsApiVersion>",
        "sites": "<sitesApiVersion>",
        "sitesExtensions": "<sitesExtensionsApiVersion>",
        "storageAccounts": "<storageAccountsApiVersion>",
        "virtualMachines": "<virtualMachinesApiVersion>",
        "virtualMachinesExtensions": "<virtualMachinesExtensionsApiVersion>",
        "virtualMachineScaleSets": "<virtualMachineScaleSetsApiVersion>",
        "virtualMachineScaleSetsExtensions": "<virtualMachineScaleSetsExtensionsApiVersion>",
        "virtualNetworks": "<virtualNetworksApiVersion>",
        "workspaces": "<workspacesApiVersion>",
        "workspacesDataSources": "<workspacesDataSourcesApiVersion>",
        "workspacesSavedSearches": "<workspacesSavedSearchesApiVersion>",
        "workspacesSavedSearchesSchedules": "<workspacesSavedSearchesSchedulesApiVersion>",
        "workspacesSavedSearchesSchedulesActions": "<workspacesSavedSearchesSchedulesActionsApiVersion>",
        "workspacesViews": "<workspacesViewsApiVersion>",
        "roleAssignments": "<roleAssignments>",
        "identities": "<identities>",
        "keyVaults": "<keyVaults>",
        "eventGrids": "<eventGrids>",
        "automationAccounts": "<automationAccounts>",
        "eventHubs": "<eventHubs>",
        "dataBricks": "<dataBricks>"
      }
    },
    "eventHubNamespaceName": {
      "value": "<eventHubNameSpace>"
    },
    "eventhubSku": {
      "value": "Standard"
    },
    "isAutoInflateEnabled": {
      "value": "True"
    },
    "maximumThroughputUnits": {
      "value": 10
    },
    "eventHubName": {
      "value": "<eventHubName>"
    },
    "messageRetentionInDays": {
      "value": 1
